{
  "name": "Purchase",
  "plural": "Purchases",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "date": {
      "type": "date",
      "required": true
    },
    "quantity": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "specie": {
      "type": "belongsTo",
      "model": "Specie",
      "foreignKey": "specie_id"
    },
    "client": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "client_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "cashier",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
